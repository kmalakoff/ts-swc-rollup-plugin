{
  "name": "ts-swc-rollup-plugin",
  "version": "1.5.7",
  "description": "A rollup plugin for swc using ts-swc-transform",
  "keywords": [
    "swc",
    "rollup",
    "plugin",
    "ts-swc-transform"
  ],
  "homepage": "https://github.com/kmalakoff/ts-swc-rollup-plugin",
  "repository": {
    "type": "git",
    "url": "git@github.com:kmalakoff/ts-swc-rollup-plugin.git"
  },
  "license": "MIT",
  "author": "Kevin Malakoff <kmalakoff@gmail.com> (https://github.com/kmalakoff)",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./*": "./*"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsds build",
    "depcheck": "depcheck",
    "deploy": "tsds deploy",
    "format": "biome check --write --unsafe src/ test/",
    "test": "tsds test:node --no-timeouts",
    "test:engines": "nvu engines npm test",
    "version": "tsds version"
  },
  "dependencies": {
    "get-tsconfig-compat": "^2.0.1",
    "rollup": "*",
    "ts-swc-transform": "^1.7.9"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.10.2",
    "depcheck": "^1.4.7",
    "homedir-polyfill": "^1.0.3",
    "ts-dev-stack": "^1.5.2"
  },
  "packageManager": "npm@11.0.0+sha512.11dff29565d2297c74e7c594a9762581bde969f0aa5cbe6f5b3644bf008a16c065ece61094d9ffbb81125be38df8e1ba43eb8244b3d30c61eb797e9a2440e3ec",
  "engines": {
    "node": ">=16"
  },
  "tsds": {
    "source": "src/index.ts"
  }
}
